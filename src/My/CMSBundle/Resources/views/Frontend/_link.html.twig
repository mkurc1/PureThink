<div id="links">
    <div class="container">
        <h3>{{ entities['title'] }}</h3>
        <ul>
            {% for key, link in entities if key != 'title' %}
                <li><a href="{{ path('article', {'locale': locale, 'slug': link['adres']}) }}">{{ link['nazwa'] }}</a></li>
            {% endfor %}
        </ul>
    </div>
</div>