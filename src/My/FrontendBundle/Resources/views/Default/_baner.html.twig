<div id="baner">
    <div class="container">
        {% for baner in baners %}
            <a href="{{ path('article', { 'locale': locale, 'slug': baner['adres'] }) }}">
                <img src="{{ asset('uploads/' ~ baner['fotografia']) }}" alt="{{ baner['nazwa'] }}" />
            </a>
        {% endfor %}
    </div>
</div>