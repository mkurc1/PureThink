{% extends "::base.html.twig" %}

{% block stylesheets %}
    {{ parent() }}
    <link rel="stylesheet" type="text/css" href="{{ asset('css/cms/article.css') }}">
    <link rel="stylesheet" type="text/css" href="{{ asset('template/fresh-style/css/main.css') }}">
{% endblock stylesheets %}

{% block javascripts %}
    {% javascripts
        '@jquery'
        '@MyFrontendBundle/Resources/public/js/*'
        output='js/compiled/frontend.js' %}
        <script src="{{ asset_url }}"></script>
    {% endjavascripts %}
    <script src="{{ asset('template/fresh-style/js/jquery.slides.min.js') }}"></script>
    <script src="{{ asset('template/fresh-style/js/main.js') }}"></script>
{% endblock javascripts %}